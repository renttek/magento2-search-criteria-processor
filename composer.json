{
  "name": "renttek/magento2-search-criteria-processor",
  "license": "proprietary",
  "description": "Generic processor for Magento 2 SearchCriteria",
  "type": "library",
  "require": {
    "php": ">=7.4",
    "magento/framework": ">=103.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.4",
    "squizlabs/php_codesniffer": "^3.5",
    "phpstan/phpstan": "^0.12.55",
    "phpmd/phpmd": "^2.9"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.magento.com/",
      "cannonical": false
    }
  ],
  "autoload": {
    "files": [
      "src/functions.php"
    ],
    "psr-4": {
      "Renttek\\SearchCriteriaProcessor\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Renttek\\SearchCriteriaProcessor\\Test\\": "test"
    }
  }
}
