{
  "name": "renttek/m2-search-criteria-processor",
  "license": "proprietary",
  "description": "Generic processor for Magento 2 SearchCriteria",
  "type": "library",
  "require": {
    "php": "~7.1.0|~7.2.0|~7.3.0",
    "magento/framework": "~101.0|~102.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.4",
    "squizlabs/php_codesniffer": "^3.5",
    "phpmd/phpmd": "^2.7",
    "phpstan/phpstan-shim": "^0.11.16"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.magento.com/"
    }
  ],
  "autoload": {
    "files": [
      "src/functions.php"
    ],
    "psr-4": {
      "Renttek\\SearchCriteriaProcessor\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Renttek\\SearchCriteriaProcessor\\Test\\": "test"
    }
  }
}
